# Discord Bot Environment Variables
# Copy this file to .env and fill in your values

# Discord Bot Configuration
DISCORD_TOKEN=DISCORD_TOKEN_PLACEHOLDER
CLIENT_ID=your_client_id_here
GUILD_ID=your_guild_id_for_testing_optional

# Bot Settings
BOT_PREFIX=!
BOT_OWNERS=your_user_id_here
DEFAULT_VOLUME=50
MAX_QUEUE_SIZE=100
PREMIUM_MAX_QUEUE_SIZE=500
COMMAND_COOLDOWN=3000
PREMIUM_COMMAND_COOLDOWN=1000
AUTO_DEPLOY_COMMANDS=true

# Spotify Integration (Optional)
SPOTIFY_CLIENT_ID=your_spotify_client_id_here
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret_here

# OpenAI Integration (Optional)
OPENAI_API_KEY=OPENAI_API_KEY_PLACEHOLDER

# Logging Level
LOG_LEVEL=info
